        <servlet-class>com.onlineshop.controller.frontend.GoMyOrdersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GoOrderDetailServlet</servlet-name>
        <servlet-class>com.onlineshop.controller.frontend.GoOrderDetailServlet</servlet-class>
